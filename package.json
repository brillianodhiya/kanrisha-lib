{
  "name": "kanrisha-lib",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/kanrisha-lib.umd.cjs",
  "module": "./dist/kanrisha-lib.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/kanrisha-lib.js",
      "require": "./dist/kanrisha-lib.umd.cjs"
